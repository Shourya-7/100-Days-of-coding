// Q24 - Write a program to calculate electricity bill based on units consumed.

#include <stdio.h>

int main(void) {
    int units;
    if (scanf("%d", &units) != 1) return 0;

    long long bill = 0;

    if (units > 0) {
        int use = units;

        int slab = (use > 100) ? 100 : use;
        bill += slab * 5;
        use -= slab;

        if (use > 0) {
            slab = (use > 100) ? 100 : use;
            bill += slab * 7;
            use -= slab;
        }
        if (use > 0) {
            slab = (use > 100) ? 100 : use;
            bill += slab * 10;
            use -= slab;
        }
        if (use > 0) {
            bill += use * 12;
        }
    }
    printf("Bill: %lld\n", bill);
    return 0;
}

