// Q25 - Write a program to implement a basic calculator using switch-case for +, -, *, /, %.

#include <stdio.h>

int main(void) {
    char op;
    double a, b;

    if (scanf(" %c", &op) != 1) return 0;
    if (scanf("%lf %lf", &a, &b) != 2) return 0;

    switch (op) {
        case '+':
            printf("%.2f\n", a + b);
            break;
        case '-':
            printf("%.2f\n", a - b);
            break;
        case '*':
            printf("%.2f\n", a * b);
            break;
        case '/':
            if (b == 0.0) printf("Error: Division by zero\n");
            else printf("%.2f\n", a / b);
            break;
        case '%': {
            long long x = (long long)a;
            long long y = (long long)b;
            if (y == 0) printf("Error: Modulo by zero\n");
            else printf("%lld\n", x % y);
            break;
        }
        default:
            printf("Invalid operator\n");
    }
    return 0;
}

