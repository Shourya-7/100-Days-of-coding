// Q22 - Write a program to find profit or loss percentage given cost price and selling price.

#include <stdio.h>
#include <math.h>

int main(void) {
    double cp, sp;
    if (scanf("%lf %lf", &cp, &sp) != 2) return 0;

    if (cp == 0.0) {
        printf("Invalid: Cost price cannot be zero\n");
        return 0;
    }

    double diff = sp - cp;
    double pct = fabs(diff) * 100.0 / cp;

    if (diff > 0)
        printf("Profit: %.2f%%\n", pct);
    else if (diff < 0)
        printf("Loss: %.2f%%\n", pct);
    else
        printf("No Profit No Loss\n");

    return 0;
}

// --------------------------------------------------------------

// Q23 - Write a program to calculate library fine based on late days.

#include <stdio.h>

int main(void) {
    int days;
    if (scanf("%d", &days) != 1) return 0;

    if (days <= 0) {
        printf("Fine: 0\n");
    } else if (days <= 5) {
        printf("Fine: %d\n", days * 2);
    } else if (days <= 10) {
        int fine = 5 * 2 + (days - 5) * 4;
        printf("Fine: %d\n", fine);
    } else if (days <= 30) {
        int fine = 5 * 2 + 5 * 4 + (days - 10) * 6;
        printf("Fine: %d\n", fine);
    } else {
        printf("Membership Cancelled\n");
    }
    return 0;
}

