// Q26 - Write a program to print numbers from 1 to n.

#include <stdio.h>

int main(void) {
    int n;
    if (scanf("%d", &n) != 1) return 0;

    if (n >= 1) {
        for (int i = 1; i <= n; i++) {
            printf("%d", i);
            if (i < n) printf(" ");
        }
    }
    printf("\n");
    return 0;
}

// --------------------------------------------------------------

// Q27 - Write a program to print the sum of the first n odd numbers.

#include <stdio.h>

int main(void) {
    int n, sum = 0, i, odd = 1;
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        sum += odd;
        odd += 2;
    }
    printf("%d\n", sum);
    return 0;
}

// --------------------------------------------------------------

// Q28 - Write a program to print the product of even numbers from 1 to n.

#include <stdio.h>

int main(void) {
    int n;
    long long product = 1;
    scanf("%d", &n);

    for (int i = 2; i <= n; i += 2) {
        product *= i;
    }
    printf("%lld\n", product);
    return 0;
}

// --------------------------------------------------------------

// Q29 - Write a program to calculate the factorial of a number.

#include <stdio.h>

int main(void) {
    int n;
    long long fact = 1;
    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {
        fact *= i;
    }
    printf("%lld\n", fact);
    return 0;
}

// --------------------------------------------------------------

// Q30 - Write a program to reverse a given number.

#include <stdio.h>

int main(void) {
    int num, rev = 0;
    scanf("%d", &num);

    while (num != 0) {
        rev = rev * 10 + (num % 10);
        num /= 10;
    }
    printf("%d\n", rev);
    return 0;
}

