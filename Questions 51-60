// Q51 - Write a program to print the following pattern:
// 5
// 45
// 345
// 2345
// 12345

#include <stdio.h>

int main(void) {
    for (int start = 5; start >= 1; --start) {
        for (int x = start; x <= 5; ++x) printf("%d", x);
        printf("\n");
    }
    return 0;
}

// --------------------------------------------------------------

// Q52 - Write a program to print the following pattern (n=10):
// .
// .
// .
// .
// .
// .
// .
// .
// .
// .
// (i.e., a vertical line of 10 dots; print n lines each with a single dot)

#include <stdio.h>

int main(void) {
    int n = 10;               // change if your judge provides/needs input
    for (int i = 0; i < n; ++i) printf(".\n");
    return 0;
}

// --------------------------------------------------------------

// Q53 - Write a program to print the following pattern (left-aligned pyramid of stars, 5 rows):
// *
// ***
// *****
// *******
// *********

#include <stdio.h>

int main(void) {
    int rows = 5;
    for (int i = 1; i <= rows; ++i) {
        int stars = 2*i - 1;
        for (int j = 0; j < stars; ++j) printf("*");
        printf("\n");
    }
    return 0;
}

// --------------------------------------------------------------

// Q54 - Write a program to print the following pattern (diamond without middle spacing):
// *
// **
// ***
// ****
// ***
// **
// *

#include <stdio.h>

int main(void) {
    int n = 4; // peak height
    for (int i = 1; i <= n; ++i) { for (int j = 1; j <= i; ++j) printf("*"); printf("\n"); }
    for (int i = n - 1; i >= 1; --i) { for (int j = 1; j <= i; ++j) printf("*"); printf("\n"); }
    return 0;
}

// --------------------------------------------------------------

// Q55 - Write a program to print all prime numbers from 1 to n.

#include <stdio.h>
#include <math.h>
int isPrime55(int x){ if(x<2) return 0; if(x%2==0) return x==2; for(int i=3;i*i<=x;i+=2) if(x%i==0) return 0; return 1; }

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int first=1;
    for(int i=2;i<=n;i++) if(isPrime55(i)){ if(!first) printf(" "); printf("%d",i); first=0; }
    printf("\n"); return 0;
}

// --------------------------------------------------------------

// Q56 - Read and print elements of a one-dimensional array.

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int a[1000];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    for(int i=0;i<n;i++){ if(i) printf(" "); printf("%d",a[i]); }
    printf("\n"); return 0;
}

// --------------------------------------------------------------

// Q57 - Find the sum of array elements.

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    long long s=0; 
    for(int i=0,x;i<n;i++){ scanf("%d",&x); s+=x; }
    printf("%lld\n",s); 
    return 0;
}

// --------------------------------------------------------------

// Q58 - Find the maximum and minimum element in an array.

#include <stdio.h>
#include <limits.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    long long mn=LLONG_MAX,mx=LLONG_MIN,x;
    for(int i=0;i<n;i++){ scanf("%lld",&x); if(x<mn) mn=x; if(x>mx) mx=x; }
    printf("Min: %lld\nMax: %lld\n",mn,mx);
    return 0;
}

// --------------------------------------------------------------

// Q59 - Count even and odd numbers in an array.

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int ev=0,od=0,x;
    for(int i=0;i<n;i++){ scanf("%d",&x); if(x%2==0) ev++; else od++; }
    printf("Even: %d\nOdd: %d\n",ev,od); 
    return 0;
}

// --------------------------------------------------------------

// Q60 - Count positive, negative, and zero elements in an array.

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int pos=0,neg=0,zero=0,x;
    for(int i=0;i<n;i++){ scanf("%d",&x); if(x>0) pos++; else if(x<0) neg++; else zero++; }
    printf("Positive: %d\nNegative: %d\nZero: %d\n",pos,neg,zero);
    return 0;
}

