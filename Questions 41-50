// Q41 - Write a program to swap the first and last digit of a number.

#include <stdio.h>

int main(void) {
    long long n;
    if (scanf("%lld", &n) != 1) return 0;

    long long sign = (n < 0) ? -1 : 1;
    n *= sign;

    if (n < 10) { printf("%lld\n", sign * n); return 0; }

    long long last = n % 10;
    long long temp = n, pow10 = 1;

    while (temp >= 10) { temp /= 10; pow10 *= 10; }
    long long first = temp;

    long long middle = (n % pow10) / 10;
    long long swapped = last * pow10 + middle * 10 + first;

    printf("%lld\n", sign * swapped);
    return 0;
}

// --------------------------------------------------------------

// Q42 - Write a program to check if a number is a perfect number.

#include <stdio.h>

int main(void) {
    long long n;
    if (scanf("%lld", &n) != 1) return 0;

    if (n <= 1) { printf("Not Perfect\n"); return 0; }

    long long sum = 1;
    for (long long i = 2; i * i <= n; i++) {
        if (n % i == 0) {
            sum += i;
            if (i != n / i) sum += n / i;
        }
    }
    if (sum == n) printf("Perfect\n");
    else          printf("Not Perfect\n");
    return 0;
}

// --------------------------------------------------------------

// Q43 - Write a program to check if a number is a strong number.

#include <stdio.h>

long long fact43(int d) {
    long long f = 1;
    for (int i = 2; i <= d; i++) f *= i;
    return f;
}

int main(void) {
    long long n;
    if (scanf("%lld", &n) != 1) return 0;

    long long temp = (n < 0) ? -n : n;
    long long sum = 0;

    if (temp == 0) sum = fact43(0);
    while (temp > 0) {
        int d = temp % 10;
        sum += fact43(d);
        temp /= 10;
    }

    if (sum == (n < 0 ? -n : n)) printf("Strong\n");
    else                         printf("Not Strong\n");
    return 0;
}

// --------------------------------------------------------------

// Q44 - Write a program to find the sum of the series: 1 + 3/4 + 5/6 + 7/8 + ... up to n terms.

#include <stdio.h>

int main(void) {
    int n;
    if (scanf("%d", &n) != 1) return 0;

    double sum = 0.0;
    for (int k = 1; k <= n; k++) {
        if (k == 1) sum += 1.0;
        else        sum += (2.0*k - 1.0) / (2.0*k);
    }
    printf("%.2f\n", sum);
    return 0;
}

// --------------------------------------------------------------

// Q45 - Write a program to find the sum of the series: 2/3 + 4/5 + 6/7 + 8/9 + ... up to n terms.

#include <stdio.h>

int main(void) {
    int n;
    if (scanf("%d", &n) != 1) return 0;

    double sum = 0.0;
    for (int k = 1; k <= n; k++) {
        double num = 2.0 * k;
        double den = 2.0 * k + 1.0;
        sum += num / den;
    }
    printf("%.2f\n", sum);
    return 0;
}

// --------------------------------------------------------------

// Q46 - Write a program to print the following pattern (5x5 solid square).
// *****
// *****
// *****
// *****
// *****

#include <stdio.h>

int main(void) {
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) printf("*");
        printf("\n");
    }
    return 0;
}

// --------------------------------------------------------------

// Q47 - Write a program to print the following pattern (right triangle).
// *
// **
// ***
// ****
// *****

#include <stdio.h>

int main(void) {
    for (int i = 1; i <= 5; i++) {
        for (int j = 1; j <= i; j++) printf("*");
        printf("\n");
    }
    return 0;
}

// --------------------------------------------------------------

// Q48 - Write a program to print the following pattern (number triangle).
// 1
// 12
// 123
// 1234
// 12345

#include <stdio.h>

int main(void) {
    for (int i = 1; i <= 5; i++) {
        for (int j = 1; j <= i; j++) printf("%d", j);
        printf("\n");
    }
    return 0;
}

// --------------------------------------------------------------

// Q49 - Write a program to print the following pattern.
// 5
// 45
// 345
// 2345
// 12345

#include <stdio.h>

int main(void) {
    for (int row = 5; row >= 1; row--) {
        for (int x = row; x <= 5; x++) printf("%d", x);
        printf("\n");
    }
    return 0;
}

// --------------------------------------------------------------

// Q50 - Write a program to print the following pattern (inverted right triangle).
// *****
// ****
// ***
// **
// *

#include <stdio.h>

int main(void) {
    for (int i = 5; i >= 1; i--) {
        for (int j = 1; j <= i; j++) printf("*");
        printf("\n");
    }
    return 0;
}

