// Q61 - Search for an element in an array using linear search.

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int a[1000]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    int key; scanf("%d",&key);
    int idx=-1; for(int i=0;i<n;i++) if(a[i]==key){ idx=i; break; }
    if(idx==-1) printf("Not Found\n"); else printf("Found at index %d\n",idx);
    return 0;
}

// --------------------------------------------------------------

// Q62 - Reverse an array without taking extra space.

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int a[1000]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    for(int l=0,r=n-1;l<r;l++,r--){ int t=a[l]; a[l]=a[r]; a[r]=t; }
    for(int i=0;i<n;i++){ if(i) printf(" "); printf("%d",a[i]); }
    printf("\n"); return 0;
}

// --------------------------------------------------------------

// Q63 - Merge two arrays.

#include <stdio.h>

int main(void){
    int n,m; if(scanf("%d",&n)!=1) return 0;
    int a[1000]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    scanf("%d",&m); int b[1000]; for(int j=0;j<m;j++) scanf("%d",&b[j]);
    for(int i=0;i<n;i++){ if(i) printf(" "); printf("%d",a[i]); }
    for(int j=0;j<m;j++){ printf(" %d",b[j]); }
    printf("\n"); return 0;
}

// --------------------------------------------------------------

// Q64 - Find the digit that occurs the most times in an integer number.

#include <stdio.h>

int main(void){
    long long n; if(scanf("%lld",&n)!=1) return 0;
    if(n<0) n=-n;
    int cnt[10]={0};
    if(n==0) cnt[0]=1;
    while(n>0){ cnt[n%10]++; n/=10; }
    int best=0;
    for(int d=1;d<=9;d++) if(cnt[d]>cnt[best]) best=d;
    printf("%d\n",best);
    return 0;
}

// --------------------------------------------------------------

// Q65 - Search in a sorted array using binary search (print index or Not Found).

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int a[100000]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    int key; scanf("%d",&key);
    int l=0,r=n-1,ans=-1;
    while(l<=r){ int mid=l+(r-l)/2; if(a[mid]==key){ ans=mid; break; } else if(a[mid]<key) l=mid+1; else r=mid-1; }
    if(ans==-1) printf("Not Found\n"); else printf("Found at index %d\n",ans);
    return 0;
}

// --------------------------------------------------------------

// Q66 - Insert an element in a sorted array at the appropriate position.

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int a[1005]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    int x; scanf("%d",&x);
    int i=n-1; while(i>=0 && a[i]>x){ a[i+1]=a[i]; i--; } a[i+1]=x; n++;
    for(int j=0;j<n;j++){ if(j) printf(" "); printf("%d",a[j]); } 
    printf("\n"); return 0;
}

// --------------------------------------------------------------

// Q67 - Insert an element in an array at a given position (0-based index).

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int a[1005]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    int pos,val; scanf("%d %d",&pos,&val);
    if(pos<0) pos=0; if(pos>n) pos=n;
    for(int i=n;i>pos;i--) a[i]=a[i-1];
    a[pos]=val; n++;
    for(int i=0;i<n;i++){ if(i) printf(" "); printf("%d",a[i]); }
    printf("\n"); return 0;
}

// --------------------------------------------------------------

// Q68 - Delete an element from an array (first occurrence of key).

#include <stdio.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int a[1005]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    int key; scanf("%d",&key);
    int pos=-1; for(int i=0;i<n;i++) if(a[i]==key){ pos=i; break; }
    if(pos!=-1){ for(int i=pos;i<n-1;i++) a[i]=a[i+1]; n--; }
    for(int i=0;i<n;i++){ if(i) printf(" "); printf("%d",a[i]); }
    printf("\n"); return 0;
}

// --------------------------------------------------------------

// Q69 - Find the second largest element in an array.

#include <stdio.h>
#include <limits.h>

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    long long first=LLONG_MIN, second=LLONG_MIN,x;
    for(int i=0;i<n;i++){
        scanf("%lld",&x);
        if(x>first){ second=first; first=x; }
        else if(x>second && x<first){ second=x; }
    }
    if(second==LLONG_MIN) printf("Not Applicable\n");
    else printf("%lld\n",second);
    return 0;
}

// --------------------------------------------------------------

// Q70 - Rotate an array to the right by k positions.

#include <stdio.h>

void rev(int a[], int l, int r){ while(l<r){ int t=a[l]; a[l]=a[r]; a[r]=t; l++; r--; } }

int main(void){
    int n; if(scanf("%d",&n)!=1) return 0;
    int a[100000]; for(int i=0;i<n;i++) scanf("%d",&a[i]);
    int k; scanf("%d",&k);
    if(n>0){ k%=n; if(k<0) k+=n; rev(a,0,n-1); rev(a,0,k-1); rev(a,k,n-1); }
    for(int i=0;i<n;i++){ if(i) printf(" "); printf("%d",a[i]); }
    printf("\n"); return 0;
}

